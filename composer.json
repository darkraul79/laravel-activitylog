{
	"name": "darkrauk79/laravel-activitylog",
	"description": "Para LoeweCampus",
	"homepage": "https://github.com/darkraul79/laravel-activitylog",
	"keywords": [],
	"authors": [
		{
			"name": "Raul Sebastian",
			"email": "darkraul@gmail.com",
			"role": "Developer"
		}
	],
	"require": {
		"php": "^7.0",
		"illuminate/config": "~5.5.0|~5.6.0|~5.7.0",
		"illuminate/database": "~5.5.0|~5.6.0|~5.7.0",
		"illuminate/support": "~5.5.0|~5.6.0|~5.7.0",
		"spatie/string": "^2.1"
	},
	"require-dev": {
		"phpunit/phpunit": "^6.2|^7.0",
		"orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0"
	},
	"autoload": {
		"psr-4": {
			"darkraul79\\Activitylog\\": "src"
		},
		"files": [
			"src/helpers.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"darkraul79\\Activitylog\\Test\\": "tests"
		}
	},
	"scripts": {
		"test": "vendor/bin/phpunit"
	},
	"license": "MIT",
	"extra": {
		"laravel": {
			"providers": [
				"darkraul79\\Activitylog\\ActivitylogServiceProvider"
			]
		}
	},
	"repositories": [
		{
			"type": "vcs",
			"url": "git@github.com:darkraul79/laravel-activitylog.git"
		}
	]
}