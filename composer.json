{
	"name": "darkraul79/laravel-activitylog",
	"description": "A very simple activity logger to monitor the users of your website or application",
	,
	"authors": [{
		"name": "Raul",
		"email": "freek@Darkraul79.be",
		"homepage": "https://Darkraul79.be",
		"role": "Developer"
	}],
	"require": {
		"php": "^7.0",
		"illuminate/config": "~5.5.0|~5.6.0|~5.7.0",
		"illuminate/database": "~5.5.0|~5.6.0|~5.7.0",
		"illuminate/support": "~5.5.0|~5.6.0|~5.7.0",
		"Darkraul79/string": "^2.1"

	},
	"require-dev": {
		"phpunit/phpunit": "^6.2|^7.0",
		"orchestra/testbench": "~3.5.0|~3.6.0|~3.7.0"
	},
	"autoload": {
		"psr-4": {
			"Darkraul79\\Activitylog\\": "src"
		},
		"files": [
			"src/helpers.php"
		]
	},
	"autoload-dev": {
		"psr-4": {
			"Darkraul79\\Activitylog\\Test\\": "tests"
		}
	},
	"scripts": {
		"test": "vendor/bin/phpunit"
	},
	"license": "MIT",
	"extra": {
		"laravel": {
			"providers": [
				"Darkraul79\\Activitylog\\ActivitylogServiceProvider"
			]
		}
	}
}